  }

transactionography_num =
  ## (number) contains a number, written in any form. [3.5.3. Numbers and
  ## Measures]
  element num {
    transactionography_macro.phraseSeq,
    transactionography_att.global.attributes,
    transactionography_att.ranging.attributes,
    ## indicates the type of numeric value.
    ## Suggested values include: 1] cardinal; 2] ordinal; 3] fraction; 4] percentage
    attribute type {
      ## absolute number, e.g. 21, 21.5
      "cardinal" | ## ordinal number, e.g. 21st
        "ordinal" | ## fraction, e.g. one half or three-quarters
        "fraction" | ## a percentage
        "percentage"
      | xsd:Name
    }?,
    ## supplies the value of the number in standard form.
    attribute value { transactionography_data.numeric }?,
    empty
  }

transactionography_measure =
  ## contains a word or phrase referring to some quantity of an object or commodity, usually comprising a number, a unit, and a commodity name. [3.5.3. Numbers and
  ## Measures]
  element measure {
    transactionography_macro.phraseSeq,
    transactionography_att.global.attributes,
    transactionography_att.measurement.attributes,
    ## specifies the type of measurement in any convenient typology.
    attribute type { transactionography_data.enumerated }?,
