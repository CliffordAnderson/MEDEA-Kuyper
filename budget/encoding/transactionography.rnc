  }

transactionography_handDesc =
  ## (description of hands) contains a description of all the different kinds of writing used in a manuscript. [10.7.2. Writing, Decoration, and Other Notations]
  element handDesc {
    (transactionography_model.pLike+
     | (transactionography_summary?, transactionography_handNote+)),
    transactionography_att.global.attributes,
    ## specifies the number of distinct hands identified within the manuscript
    attribute hands { transactionography_data.count }?,
    empty
  }

transactionography_typeDesc =
  ## contains a description of the typefaces or other aspects of the printing of an incunable or other printed source. [10.7.2.1. Writing]
  element typeDesc {
    (transactionography_model.pLike+
     | (transactionography_summary?, transactionography_typeNote+)),
    transactionography_att.global.attributes,
    empty
  }

transactionography_scriptDesc =
  ## contains a description of the scripts used in a manuscript or similar source. [10.7.2.1. Writing]
  element scriptDesc {
    (transactionography_model.pLike+
     | (transactionography_summary?, transactionography_scriptNote+)),
    transactionography_att.global.attributes,
    empty
  }

transactionography_musicNotation =
  ## contains description of type of musical notation. [10.7.2. Writing, Decoration, and Other Notations]
  element musicNotation {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    empty
  }

transactionography_decoDesc =
  ## (decoration description) contains a description of the decoration of a manuscript, either as a sequence of paragraphs, or as a sequence of topically organized decoNote elements. [10.7.3. Bindings, Seals, and Additional Material]
  element decoDesc {
    (transactionography_model.pLike+
     | (transactionography_summary?, transactionography_decoNote+)),
    transactionography_att.global.attributes,
    empty
  }

transactionography_decoNote =
  ## (note on decoration) contains a note describing either a decorative component of a manuscript, or a fairly homogenous class of such components. [10.7.3. Bindings, Seals, and Additional Material]
  element decoNote {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    transactionography_att.typed.attributes,
    empty
  }

transactionography_additions =
  ## contains a description of any significant additions found within a manuscript, such as marginalia or other annotations.  [10.7.2. Writing, Decoration, and Other Notations]
  element additions {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    empty
  }

transactionography_bindingDesc =
  ## (binding description) describes the present and former bindings of a manuscript, either as a series of paragraphs or as a series of distinct binding elements, one for each binding of the manuscript. [10.7.3.1. Binding Descriptions]
  element bindingDesc {
    ((transactionography_model.pLike
      | transactionography_decoNote
      | transactionography_condition)+
     | transactionography_binding+),
    transactionography_att.global.attributes,
    empty
  }

transactionography_binding =
  ## contains a description of one binding, i.e. type of covering, boards, etc. applied to a manuscript. [10.7.3.1. Binding Descriptions]
  element binding {
    (transactionography_model.pLike
     | transactionography_condition
     | transactionography_decoNote)+,
    transactionography_att.global.attributes,
    transactionography_att.datable.attributes,
    ## specifies whether or not the binding is contemporary with the majority of its contents
    attribute contemporary { transactionography_data.xTruthValue }?,
    empty
  }

transactionography_sealDesc =
  ## (seal description) describes the seals or other external items attached to a manuscript, either as a series of paragraphs or as a series of distinct seal elements, possibly with additional decoNotes. [10.7.3.2. Seals]
  element sealDesc {
    (transactionography_model.pLike+
     | (transactionography_summary?,
        (transactionography_decoNote
         | transactionography_seal
         | transactionography_condition)+)),
    transactionography_att.global.attributes,
    empty
  }

transactionography_seal =
  ## contains a description of one seal or similar attachment applied to a manuscript. [10.7.3.2. Seals]
  element seal {
    (transactionography_model.pLike | transactionography_decoNote)+,
    transactionography_att.global.attributes,
    transactionography_att.typed.attributes,
    transactionography_att.datable.attributes,
    ## specifies whether or not the seal is contemporary with the item to which it is affixed
    attribute contemporary { transactionography_data.xTruthValue }?,
    empty
  }

transactionography_accMat =
  ## (accompanying material) contains details of any significant additional material which may be closely associated with the manuscript being described, such as non-contemporaneous documents or fragments bound in with the manuscript at some earlier historical period. [10.7.3.3. Accompanying Material]
  element accMat {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    transactionography_att.typed.attributes,
    empty
  }

transactionography_history =
  ## groups elements describing the full history of a manuscript or manuscript part. [10.8. History]
  element history {
    (transactionography_model.pLike+
     | (transactionography_summary?,
        transactionography_origin?,
        transactionography_provenance*,
        transactionography_acquisition?)),
    transactionography_att.global.attributes,
    empty
  }

transactionography_origin =
  ## contains any descriptive or other information concerning the origin of a manuscript or manuscript part.  [10.8. History]
  element origin {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    transactionography_att.editLike.attributes,
    transactionography_att.datable.attributes,
    empty
  }

transactionography_provenance =
  ## contains any descriptive or other information concerning a single identifiable episode during the history of a manuscript or manuscript part, after its creation but before its acquisition.  [10.8. History]
  element provenance {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    transactionography_att.datable.attributes,
    transactionography_att.typed.attributes,
    transactionography_att.responsibility.attributes,
    empty
  }

transactionography_acquisition =
  ## contains any descriptive or other information concerning the process by which a manuscript or manuscript part entered the holding institution.  [10.8. History]
  element acquisition {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    transactionography_att.datable.attributes,
    empty
  }

transactionography_additional =
  ## groups additional information, combining bibliographic information about a manuscript, or surrogate copies of it with curatorial or administrative information.  [10.9. Additional information]
  element additional {
    (transactionography_adminInfo?,
     transactionography_surrogates?,
     transactionography_listBibl?),
    transactionography_att.global.attributes,
    empty
  }

transactionography_adminInfo =
  ## (administrative information) contains information about the present custody and availability of the manuscript, and also about the record description itself.  [10.9.1. Administrative information]
  element adminInfo {
    (transactionography_recordHist?,
     transactionography_availability?,
     transactionography_custodialHist?,
     transactionography_model.noteLike?),
    transactionography_att.global.attributes,
    empty
  }

transactionography_recordHist =
  ## (recorded history) provides information about the source and revision status of the parent manuscript description itself.  [10.9.1. Administrative information]
  element recordHist {
    (transactionography_model.pLike+
     | (transactionography_source, transactionography_change*)),
    transactionography_att.global.attributes,
    empty
  }

transactionography_source =
  ## describes the original source for the information contained with a manuscript description.  [10.9.1.1. Record History]
  element source {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    empty
  }

transactionography_custodialHist =
  ## (custodial history) contains a description of a manuscript's custodial history, either as running prose or as a series of dated custodial events.  [10.9.1.2. Availability and Custodial History]
  element custodialHist {
    (transactionography_model.pLike+ | transactionography_custEvent+),
    transactionography_att.global.attributes,
    empty
  }

transactionography_custEvent =
  ## (custodial event) describes a single event during the custodial history of a manuscript. [10.9.1.2. Availability and Custodial History]
  element custEvent {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    transactionography_att.datable.attributes,
    transactionography_att.typed.attributes,
    empty
  }

transactionography_surrogates =
  ## contains information about any representations of the manuscript being described which may exist in the holding institution or elsewhere.  [10.9. Additional information]
  element surrogates {
    transactionography_macro.specialPara,
    transactionography_att.global.attributes,
    empty
  }

transactionography_msPart =
  ## (manuscript part) contains information about an originally distinct manuscript or part of a manuscript, now forming part of a composite manuscript. [10.10. Manuscript Parts]
  element msPart {
    ((transactionography_altIdentifier
      | transactionography_msIdentifier),
     transactionography_model.headLike*,
     (transactionography_model.pLike+
      | (transactionography_msContents?,
         transactionography_physDesc?,
         transactionography_history?,
         transactionography_additional?,
         transactionography_msPart*))),
    transactionography_att.global.attributes,
    empty
  }
transactionography_att.datable.custom.attributes =
  transactionography_att.datable.custom.attribute.when-custom,
  transactionography_att.datable.custom.attribute.notBefore-custom,
  transactionography_att.datable.custom.attribute.notAfter-custom,
  transactionography_att.datable.custom.attribute.from-custom,
  transactionography_att.datable.custom.attribute.to-custom,
  transactionography_att.datable.custom.attribute.datingPoint,
  transactionography_att.datable.custom.attribute.datingMethod
transactionography_att.datable.custom.attribute.when-custom =
  ## supplies the value of a date or time in some standard form.
  attribute when-custom {
    list { transactionography_data.word+ }
  }?
transactionography_att.datable.custom.attribute.notBefore-custom =
  ## specifies the earliest possible date for the event in some custom standard form.
  attribute notBefore-custom {
    list { transactionography_data.word+ }
  }?
transactionography_att.datable.custom.attribute.notAfter-custom =
  ## specifies the latest possible date for the event in some custom standard form.
  attribute notAfter-custom {
    list { transactionography_data.word+ }
  }?
transactionography_att.datable.custom.attribute.from-custom =
  ## indicates the starting point of the period in some standard form.
  attribute from-custom {
    list { transactionography_data.word+ }
  }?
transactionography_att.datable.custom.attribute.to-custom =
  ## indicates the ending point of the period in some standard form.
  attribute to-custom {
    list { transactionography_data.word+ }
  }?
transactionography_att.datable.custom.attribute.datingPoint =
  ## supplies a pointer to some location defining a named point in time with reference to which the datable item is understood to have occurred
  attribute datingPoint { transactionography_data.pointer }?
transactionography_att.datable.custom.attribute.datingMethod =
  ## supplies a pointer to a calendar element or other means of interpreting the values of the custom dating attributes.
  attribute datingMethod { transactionography_data.pointer }?
transactionography_att.datable.iso.attributes =
  transactionography_att.datable.iso.attribute.when-iso,
  transactionography_att.datable.iso.attribute.notBefore-iso,
  transactionography_att.datable.iso.attribute.notAfter-iso,
  transactionography_att.datable.iso.attribute.from-iso,
  transactionography_att.datable.iso.attribute.to-iso
transactionography_att.datable.iso.attribute.when-iso =
  ## supplies the value of a date or time in a standard form.
  attribute when-iso { transactionography_data.temporal.iso }?
transactionography_att.datable.iso.attribute.notBefore-iso =
  ## specifies the earliest possible date for the event in standard form, e.g. yyyy-mm-dd.
  attribute notBefore-iso { transactionography_data.temporal.iso }?
transactionography_att.datable.iso.attribute.notAfter-iso =
  ## specifies the latest possible date for the event in standard form, e.g. yyyy-mm-dd.
  attribute notAfter-iso { transactionography_data.temporal.iso }?
transactionography_att.datable.iso.attribute.from-iso =
  ## indicates the starting point of the period in standard form.
  attribute from-iso { transactionography_data.temporal.iso }?
transactionography_att.datable.iso.attribute.to-iso =
  ## indicates the ending point of the period in standard form.
  attribute to-iso { transactionography_data.temporal.iso }?

transactionography_persName =
  ## (personal name) contains a proper noun or proper-noun phrase referring to a person, possibly including one or more of the person's forenames, surnames, honorifics, added names, etc. [13.2.1. Personal Names]
  element persName {
    transactionography_macro.phraseSeq,
    transactionography_att.global.attributes,
    transactionography_att.datable.attributes,
    transactionography_att.editLike.attributes,
    transactionography_att.personal.attributes,
    transactionography_att.typed.attributes,
    empty
  }

transactionography_placeName =
  ## contains an absolute or relative place name.  [13.2.3. Place Names]
  element placeName {
    transactionography_macro.phraseSeq,
    transactionography_att.datable.attributes,
    transactionography_att.editLike.attributes,
    transactionography_att.global.attributes,
    transactionography_att.personal.attributes,
    transactionography_att.typed.attributes,
    empty
  }
transactionography_att.transferable.attributes =
  transactionography_att.source.attributes,
  transactionography_att.transferable.attribute.fra,
  transactionography_att.transferable.attribute.til,
  transactionography_att.transferable.attribute.placeRef
transactionography_att.transferable.attribute.fra =
  ##  (from) 
  attribute fra { transactionography_data.pointer }?
transactionography_att.transferable.attribute.til =
  ##  (to) 
  attribute til { transactionography_data.pointer }?
transactionography_att.transferable.attribute.placeRef =
  ##  (ptr to place where transfer occured) 
  attribute placeRef { transactionography_data.pointer }?
transactionography_att.iou.attributes =
  transactionography_att.iou.attribute.heldBy,
  transactionography_att.iou.attribute.owedTo
transactionography_att.iou.attribute.heldBy =
  ## indicates the entity (typically a bank or person, but possibly a group of people, corporation, government body, fictional character, etc.) that holds or held the account
  attribute heldBy { transactionography_data.pointer }?
transactionography_att.iou.attribute.owedTo =
  ## indicates the entity (typically a person, but may also be a bank, group of people, corporation, government body, fictional character, etc.) that owns or owned the account
  attribute owedTo { transactionography_data.pointer }?

transactionography_listTransaction =
  element ns2:listTransaction {
    (transactionography_model.headLike
     | transactionography_model.global)*,
    (transactionography_transaction
     | transactionography_listTransaction
     | transactionography_model.global)+,
    transactionography_att.global.attributes,
    empty
  }

transactionography_transaction =
  element ns2:transaction {
    (transactionography_model.labelLike
     | transactionography_idno
     | transactionography_model.global)*,
    (transactionography_attested | transactionography_model.global)*,
    (transactionography_transfer
     | transactionography_transferGrp
     | transactionography_model.global)*,
    transactionography_att.global.attributes,
    transactionography_att.datable.attributes,
    transactionography_att.transferable.attributes,
    empty
  }

transactionography_transferGrp =
  element ns2:transferGrp {
    (transactionography_transfer
     | transactionography_transferGrp
     | transactionography_model.global)+,
    transactionography_att.global.attributes,
    transactionography_att.datable.attributes,
    transactionography_att.transferable.attributes,
    ## Suggested values include: 1] group (grouped) ; 2] sequence (sequenced) ; 3] choice (alternates) 
    attribute org {
      ##  (grouped) i.e., unordered set — the assertion is that all the child transfers occured, but the sequence in which they occured is either  * simultaneous,  * unknown,  * unimportant, or  * not being asserted.
      "group" | ##  (sequenced) i.e., chronologically ordered set — the assertion is that all the child transfers occured, in the order specified.
        "sequence" | ##  (alternates) I.e., the assertion is that only one child transfer occured.
        "choice"
      | xsd:Name
    }?,
    empty
  }

transactionography_transfer =
  ## A transfer of something of value from one entity (typically a person, but possibly a group of people, corporation, government body, fictional character, etc.) to another.
  element ns2:transfer {
    (transactionography_model.glossLike
     | transactionography_model.measureLike
     | transactionography_model.global
     | transactionography_attested)*,
    transactionography_att.global.attributes,
    transactionography_att.datable.attributes,
    transactionography_att.transferable.attributes,
    empty
  }

transactionography_attested =
  ## Groups together pointers (using target of ptr or ref) to the constructs that support the existence of the transaction.
  element ns2:attested {
    ((transactionography_ptr
      | transactionography_ref
      | transactionography_model.glossLike
      | transactionography_model.global)*)
    >> sch:pattern [
         id = "attested-constraint-sourceORattested"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:account|tei:transaction|tei:transfer"
           "\x{a}" ~
           "		             "
           sch:report [
             test = "@source and child::fr:attested"
             "\x{a}" ~
             "		   Either the 'source' attribute or the 'fr:attested'\x{a}" ~
             "		   element may be used to point to the encodings of the\x{a}" ~
             "		   documents that attest to the current account, but not\x{a}" ~
             "		   both.\x{a}" ~
             "		 "
           ]
           "\x{a}" ~
           "	           "
         ]
         "\x{a}" ~
         "         "
       ],
    transactionography_att.global.attributes,
    empty
  }

transactionography_listAccount =
  element ns2:listAccount {
    (transactionography_model.headLike
     | transactionography_model.global)*,
    (transactionography_account
     | transactionography_listAccount
     | transactionography_model.global)+,
    transactionography_att.global.attributes,
    transactionography_att.typed.attributes,
    transactionography_att.iou.attributes,
    empty
  }

transactionography_account =
  element ns2:account {
    (transactionography_model.labelLike
     | transactionography_idno
     | transactionography_model.glossLike
     | transactionography_model.global)*,
    (transactionography_attested | transactionography_model.global)*,
    (transactionography_account | transactionography_model.global)*,
    transactionography_att.global.attributes,
    transactionography_att.typed.attributes,
    transactionography_att.datable.attributes,
    transactionography_att.source.attributes,
    transactionography_att.iou.attributes,
    empty
  }
start =
  transactionography_TEI
  | transactionography_listTransaction
  | transactionography_listAccount
